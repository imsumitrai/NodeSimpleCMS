<!DOCTYPE html>
<html>
  <head>
        <% include ../partial/header %>
  </head>
  <body>
        <% include ../partial/navbar %>
      <div class="container">
          <div class="row">
              <div class="col-sm-3">

              </div>
              <div class="col-sm-6">
                  <table>
                      <thead>
                          <tr>
                              <th>Name</th>
                              <th>Email</th>
                          </tr>
                      </thead>
                      <tbody>
                        <% for(var i=0; i< users.length; i++){ %>
                            <tr>
                                <td><a href="/user/<%= users[i]._id %>"><%= users[i].name %></a></td>
                                <td><%= users[i].email %></td>
                            </tr>
                        <% } %>
                      </tbody>
                  </table>
              </div>
              <div class="col-sm-3">

              </div>
          </div>
      </div>
      
      <% include ../partial/footer %>
  </body>
</html>
