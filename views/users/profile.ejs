<!DOCTYPE html>
<html>
  <head>
        <% include ../partial/header %>
  </head>
  <body>
        <% include ../partial/navbar %>
      <div class="container">
          <div class="row">
              <div class="col-sm-3">

              </div>
              <div class="col-sm-6">
                    <form method="POST">
                        <h1><%= title %></h1>
                        <%if(messages.error){ %>
                            <div class="alert alert-danger" role="alert">
                                <%= messages.error %>
                            </div>
                        <% } %>
                        <%if(messages.status){ %>
                            <div class="alert alert-danger" role="alert">
                                <%= messages.status %>
                            </div>
                        <% } %>
                        <input type="hidden" name="_csrf" value="<%= csrftoken %>">
                        <div class="form-group">
                            <label>First Name</label>
                            <input type="name" name="first_name" class="form-control" placeholder="Enter First Name" value="<%= user.first_name %>" disabled>
                        </div>
                        <div class="form-group">
                            <label>Last Name</label>
                            <input type="name" name="last_name" class="form-control" placeholder="Enter Last Name" value="<%= user.last_name %>" disabled>
                        </div>
                    </form>
              </div>
              <div class="col-sm-3">

              </div>
          </div>
      </div>
      
      <% include ../partial/footer %>
  </body>
</html>
